<h1>Hangman</h1>
<p>Try to guess the secret word by typing single letters!</p>



<div class="nav-nav tabs">
<ul class="breadcrumb">

<%
for i in [*'a'..'z']
%>
<button onclick="processLetter(<%= i %>)">
<%= i %>
</button>
<span class="divider">         </span>

<%end%>

</ul>
</div>

<%= @board.join %>

<script>
function processLetter(letter) {

    return letter
}
</script>